<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="179.0" prefWidth="374.0" xmlns="http://javafx.com/javafx/17.0.12"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.agapovla.controller.car.CarController">

    <Label layoutX="49.0" layoutY="21.0" style="-fx-font-size: 18;" text="Меню редактирования парка авто"/>

    <ListView fx:id="carList"
              layoutX="40.0" layoutY="50.0"
              prefWidth="520.0"
              prefHeight="300.0"/>

    <Button layoutX="40.0" layoutY="360.0" mnemonicParsing="false" onAction="#onAddCar"
            text="Добавить"/>
    <Button layoutX="120.0" layoutY="360.0" mnemonicParsing="false" onAction="#onEditCar"
            text="Изменить"/>
    <Button layoutX="200.0" layoutY="360.0" mnemonicParsing="false" onAction="#onDeleteCar"
            text="Удалить"/>

</AnchorPane>
